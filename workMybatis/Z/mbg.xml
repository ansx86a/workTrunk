<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">


<generatorConfiguration>
	<!--数据库驱动jar -->
	<classPathEntry
		location="z:/sqljdbc42.jar" />

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<!--去除注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!--数据库连接 -->
		<jdbcConnection driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver"
			connectionURL="jdbc:sqlserver://127.0.0.1;DatabaseName=ANZ_WORKFLOW"
			userId="sa" password="sa">
		</jdbcConnection>
		<!--默认false Java type resolver will always use java.math.BigDecimal if 
			the database column is of type DECIMAL or NUMERIC. -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建 使用Maven生成在target目录下，会自动创建） -->
		<javaModelGenerator targetPackage="model"
			targetProject="z:/1">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!--生成SQLMAP文件 -->
		<sqlMapGenerator targetPackage="sqlmap" targetProject="z:/1">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="dao" targetProject="z:/1">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!--对应数据库表 mysql可以加入主键自增 字段命名 忽略某字段等 -->
		<!-- <table tableName="USER" domainObjectName="User" > -->
		<!-- </table> -->
		<table tableName="config" domainObjectName="Config"  />
		<table tableName="credit_card_case" domainObjectName="CreditCardCase" />
	</context>
</generatorConfiguration>